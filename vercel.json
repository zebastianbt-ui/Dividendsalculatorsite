{
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    { 
      "source": "/(.*)\\.html", 
      "headers": [
        { "key": "Cache-Control", "value": "no-store" }
      ] 
    },
    { 
      "source": "/", 
      "headers": [
        { "key": "Cache-Control", "value": "no-store" }
      ] 
    }
  ]
}